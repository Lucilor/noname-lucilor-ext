{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "ES2019",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "lib": ["ESNext", "DOM"],
    "moduleResolution": "Node",
    "strict": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "noImplicitReturns": true,
    "skipLibCheck": true,
    "paths": {
      "@/*": ["src/*"]
    },
    "types": ["vite/client", "noname-typings"]
  },
  "include": ["src"],
  "references": [{"path": "./tsconfig.node.json"}]
}
